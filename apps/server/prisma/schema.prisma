generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model vi_publicacion{

  idPublicacion Int @id @default(autoincrement())
  titulo String
  urlImagen String?
  descripcion String
  fechaCreacion DateTime @default(now())
  fechaPublicacion DateTime
  fechaUltimaModificacion DateTime
  estaActivo Boolean
}


