{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": ["NEXT_PUBLIC_SERVER_URL"]
    },
    "lint": {},
    "type-check": {},
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "db:push": {
      "cache": false
    }
  }
}
